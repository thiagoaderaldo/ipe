	<queryString>
		<![CDATA[SELECT id_ser,nome,COUNT(*) as qtd_por_regional FROM atendimentos inner join ser
ON id_ser = ser.id
WHERE YEAR(data_solicitacao) = EXTRACT(YEAR FROM CURRENT_DATE)
AND  
RIGHT(data_solicitacao,5) BETWEEN '12-21' AND '03-20'
GROUP BY id_ser 
ORDER BY  COUNT(id_ser)  DESC;]]>
	</queryString>
