tipologias t
ON 
a.id_tipologia = t.id
WHERE YEAR(data_solicitacao) = EXTRACT(YEAR FROM CURRENT_DATE)
AND
RIGHT(data_solicitacao,5) BETWEEN '01-01' AND '06-30'
GROUP BY a.id_tipologia ORDER BY COUNT(a.id_tipologia) DESC;]]>
	</queryString>
