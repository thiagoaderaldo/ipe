	<parameter name="QUERY_PARAMETER" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     atendimentos.`protocolo` AS atendimentos_protocolo,
     atendimentos.`titulo` AS atendimentos_titulo,
     atendimentos.`procedencia` AS atendimentos_procedencia,
     atendimentos.`prot_procedencia` AS atendimentos_prot_procedencia,
     atendimentos.`data_solicitacao` AS atendimentos_data_solicitacao,
     atendimentos.`hora_solicitacao` AS atendimentos_hora_solicitacao,
     atendimentos.`localizacao` AS atendimentos_localizacao,
     atendimentos.`num_casa` AS atendimentos_num_casa,
     atendimentos.`ponto_referencia` AS atendimentos_ponto_referencia,
     atendimentos.`id_ser` AS atendimentos_id_ser,
     atendimentos.`id_bairro` AS atendimentos_id_bairro,
     atendimentos.`solicitante` AS atendimentos_solicitante,
     atendimentos.`tel_solicitante` AS atendimentos_tel_solicitante,
     atendimentos.`desc_inicial` AS atendimentos_desc_inicial,
     atendimentos.`id_estatus` AS atendimentos_id_estatus,
     atendimentos.`desc_final` AS atendimentos_desc_final,
     atendimentos.`id_tipologia` AS atendimentos_id_tipologia,
     atendimentos.`data_conclusao` AS atendimentos_data_conclusao,
     atendimentos.`observacao` AS atendimentos_observacao,
     atendimentos.`hora_conclusao` AS atendimentos_hora_conclusao,
     atendimentos.`equipe` AS atendimentos_equipe,
     atendimentos.`responsavel` AS atendimentos_responsavel,
     atendimentos.`vtr` AS atendimentos_vtr,
     atendimentos.`proprietario_residencia` AS atendimentos_proprietario_residencia,
     atendimentos.`qtd_comodos_casa` AS atendimentos_qtd_comodos_casa,
     atendimentos.`qtd_pessoas_atingidas` AS atendimentos_qtd_pessoas_atingidas,
     t.nome as nome_tipologia,
     s.nome as nome_ser,
     b.nome as nome_bairro
FROM
     `atendimentos` atendimentos
INNER JOIN
      tipologias t
ON 
      atendimentos.id_tipologia = t.id
INNER JOIN
      ser s
ON
      atendimentos.id_ser = s.id
INNER JOIN
      bairros b
ON 
      atendimentos.id_bairro = b.id
WHERE 
atendimentos.protocolo = $P{QUERY_PARAMETER}]]>
	</queryString>
